{
    "compilerOptions": {
      "strict": false,
      "esModuleInterop": true,
      "allowJs": true,
      "checkJs": false,
      "skipLibCheck": true,
      "lib": ["dom", "es2015"],
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src"]
  }
  